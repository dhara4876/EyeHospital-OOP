        </columns>
      </TableView>
      <TextField fx:id="expiredMedTextField" layoutX="363.0" layoutY="135.0" promptText="Expired Medicine" />
      <Button layoutX="363.0" layoutY="175.0" mnemonicParsing="false" onMouseClicked="#removeExpiredMedBtnOnClick" text="Click to Remove" />
      <Button layoutX="452.0" layoutY="342.0" mnemonicParsing="false" onMouseClicked="#dashboardBtnOnClick" text="Click to Remove" />
   </children>
